!type: pcfg-language
!name: Nickname
!author: Iguanotron
!description: Melodramatic nicknames
!root: moniker

moniker:
  7 $animal-moniker
  6 $object-moniker
  4 $profession-moniker
  1 $two-part-moniker
  1 $celestial-force-moniker
  1 $quality
  0.6 $verb

animal-moniker:
  10 $animal
  7 ["The " $animal]

object-moniker:
  10 $object
  7 [$object-pluralizable s]
  1 [$adjective " " $object]
  0.5 [$count " " $object-pluralizable s]

object:
  4 $object-pluralizable
  3 $object-nonpluralizable

two-part-moniker: [$quality-prefix $feature-suffix]

profession-moniker:
  2 $profession
  1 ["The " $profession]

celestial-force-moniker: [$force " " $action]

